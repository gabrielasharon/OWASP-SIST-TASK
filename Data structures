//Linked list :
class Node:
   def __init__(self, dataval=None):
      self.dataval = dataval
      self.nextval = None
      
      class SLinkedList:
   def __init__(self):
      self.headval = None
      
      
 //Doubly Linked List :
 class Node:
    def __init__(self, dataval=None):
        self.dataval = dataval
        self.nextvnextal = None
 
class LinkedList:
    def __init__(self):
        self.headval = None
        
 //Circular Linked List :
 class Node:
    def __init__(self, data):
        self.data = data
        self.next = None


class CircularLinkedList:
    def __init__(self):
        self.last = None
 
 
//Stack :
class Stack:
   def __init__(self):
      self.stack = []
      
      
//Queue : 
class Queue:
   def __init__(self):
      self.queue = list()
     
